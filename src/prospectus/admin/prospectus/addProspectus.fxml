<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="715.0" stylesheets="@../courses/managecourses.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="prospectus.admin.prospectus.AddProspectusController">
   <children>
      <Pane fx:id="backgroundPane" prefHeight="550.0" prefWidth="715.0" style="-fx-background-color: white;">
         <children>
            <Group layoutX="80.0" layoutY="-36.0" />
            <Button layoutX="258.0" layoutY="498.0" mnemonicParsing="false" onMouseClicked="#addProspectusHandler" prefHeight="30.0" prefWidth="200.0" text="Add">
               <font>
                  <Font name="Arial Black" size="12.0" />
               </font>
            </Button>
            <Label layoutX="193.0" layoutY="89.0" text="Program" textFill="#170101">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Label>
            <Label prefHeight="50.0" prefWidth="715.0" style="-fx-font-weight: bold; -fx-font-size: 18;" styleClass="header" stylesheets="@../students/managestudents.css" text="Add Prospectus">
               <font>
                  <Font name="Cambria Bold" size="20.0" />
               </font>
            </Label>
            <ImageView fitHeight="30.0" fitWidth="30.0" layoutX="14.0" layoutY="10.0" onMouseClicked="#returnHandler" pickOnBounds="true">
               <image>
                  <Image url="@../../images/returnButton.png" />
               </image>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <effect>
                  <ColorAdjust brightness="1.0" />
               </effect>
            </ImageView>
            <ComboBox fx:id="selectProgramComboBox" layoutX="108.0" layoutY="110.0" prefWidth="220.0" promptText="Select Program" />
            <ListView fx:id="courseAddedList" layoutX="393.0" layoutY="90.0" prefHeight="384.0" prefWidth="284.0" />
            <Label layoutX="480.0" layoutY="71.0" style="-fx-font-weight: bold;" text="Course Added List" textFill="#170101">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Label>
            <Label layoutX="198.0" layoutY="156.0" text="Course" textFill="#170101">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Label>
            <ComboBox fx:id="selectCoursesComboBox" layoutX="108.0" layoutY="177.0" prefWidth="220.0" promptText="Select Courses" />
            <Button layoutX="152.0" layoutY="223.0" mnemonicParsing="false" onMouseClicked="#addCourseHandler" prefHeight="30.0" prefWidth="130.0" text="Add Course">
               <font>
                  <Font name="Arial Black" size="12.0" />
               </font>
            </Button>
            <Label layoutX="179.0" layoutY="280.0" text="Effective Year" textFill="#170101">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="effectiveYearField" layoutX="108.0" layoutY="303.0" prefWidth="220.0" promptText="eg. 2020-2021" />
            <HBox layoutX="120.0" layoutY="376.0" style="-fx-spacing: 10;">
               <children>
                  <RadioButton fx:id="active" mnemonicParsing="false" text="Active" />
                  <RadioButton fx:id="inactive" mnemonicParsing="false" text="Inactive" />
                  <RadioButton fx:id="archive" mnemonicParsing="false" text="Archive" />
               </children>
            </HBox>
            <Label layoutX="200.0" layoutY="351.0" text="Status" textFill="#170101">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Label>
            <Label layoutX="191.0" layoutY="411.0" text="Created By" textFill="#170101">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="createdByField" layoutX="112.0" layoutY="435.0" prefWidth="220.0" promptText="eg. 2020-2021" />
         </children>
      </Pane>
   </children>
</AnchorPane>
